<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.price %></td>
        <td><%= item.quantity %></td>
        <td><%= link_to 'Show', [:admins, item] %></td>
        <td><%= link_to 'Edit', edit_admins_item_path(item) %></td>
        <td><%= link_to 'Destroy', [:admins, item], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_admins_item_path %>
