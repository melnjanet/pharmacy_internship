<p id="notice"><%= notice %></p>

<h1>Generics Names </h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @generic_names.each do |generic_name| %>
      <tr>
        <td><%= generic_name.title %></td>
        <td><%= link_to 'Show', [:admins, generic_name] %></td>
        <td><%= link_to 'Edit', edit_admins_generic_name_path(generic_name) %></td>
        <td><%= link_to 'Destroy', [:admins, generic_name], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Generic Name', new_admins_generic_name_path %>
